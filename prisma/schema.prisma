generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}


model Autogram {
  id        Int     @id @default(autoincrement())
  content   String?
  time      String?
  createdAt DateTime @default(now())
  name      String?
  prefix    String?
  zConnector String?

}